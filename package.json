{
  "name": "@cps/elements",
  "description": "Web Components de ponta, sem complicação.",
  "version": "0.1.0",
  "homepage": "https://github.com/cpsrepositorio/cps-elements",
  "author": "Erick Petrucelli",
  "license": "MIT",
  "customElements": "dist/custom-elements.json",
  "web-types": "dist/web-types.json",
  "type": "module",
  "types": "dist/elements.d.ts",
  "exports": {
    ".": {
      "types": "./dist/elements.d.ts",
      "import": "./dist/elements.js"
    },
    "./dist/themes/*": "./dist/themes/*",
    "./dist/components/*": "./dist/components/*",
    "./dist/utilities/*": "./dist/utilities/*",
    "./dist/react": "./dist/react/index.js",
    "./dist/react/*": "./dist/react/*",
    "./dist/translations/*": "./dist/translations/*"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "web components",
    "custom elements",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpsrepositorio/cps-elements.git"
  },
  "bugs": {
    "url": "https://github.com/cpsrepositorio/cps-elements/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/ErickPetru"
  },
  "scripts": {
    "start": "node scripts/build.js --bundle --serve",
    "build": "node scripts/build.js --bundle --types --copydir \"docs/dist\"",
    "verify": "npm run prettier:check && npm run lint:check && npm run build && npm run test",
    "prepublishOnly": "npm run verify",
    "prettier:check": "prettier --check --loglevel warn .",
    "prettier:fix": "prettier --write --loglevel warn .",
    "lint:check": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "ts:check": "tsc --noEmit --project ./tsconfig.json",
    "spell:check": "cspell \"**/*.{js,ts,json,html,css,md}\" --no-progress",
    "create": "plop --plopfile scripts/plop/plopfile.js",
    "test": "web-test-runner --group default",
    "test:component": "web-test-runner -- --watch --group",
    "test:watch": "web-test-runner --watch --group default",
    "list-outdated-dependencies": "npm-check-updates --format repo --peer",
    "update-dependencies": "npm-check-updates --peer -u && npm install"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.1",
    "@lit-labs/react": "^1.1.1",
    "lit": "^2.6.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.8",
    "@iconify-json/fluent": "^1.1.18",
    "@open-wc/testing": "^3.1.7",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-commands": "^0.6.5",
    "@web/test-runner-playwright": "^0.9.0",
    "browser-sync": "^2.27.11",
    "cem-plugin-vs-code-custom-data-generator": "^1.4.1",
    "chalk": "^5.2.0",
    "command-line-args": "^5.2.1",
    "comment-parser": "^1.3.1",
    "cspell": "^6.26.3",
    "del": "^7.0.0",
    "download": "^8.0.0",
    "esbuild": "0.16.17",
    "eslint": "^8.34.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.2",
    "eslint-plugin-lit-a11y": "^2.3.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-wc": "^1.4.0",
    "front-matter": "^4.0.2",
    "get-port": "^6.1.2",
    "globby": "^13.1.3",
    "husky": "^8.0.3",
    "jsonata": "^2.0.1",
    "lint-staged": "^13.1.2",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.10.0",
    "marked": "^4.2.12",
    "npm-check-updates": "^16.7.5",
    "open": "^8.4.1",
    "pascal-case": "^3.1.2",
    "plop": "^3.1.2",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "recursive-copy": "^2.0.14",
    "sinon": "^15.0.1",
    "source-map": "^0.7.4",
    "strip-css-comments": "^5.0.0",
    "tslib": "^2.5.0",
    "typescript": "4.9.5",
    "user-agent-data-types": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --max-warnings 0 --cache --fix",
      "prettier --write"
    ]
  }
}
